/* pages/quanbu/quanbu.wxss */
.headfont{
  margin-top: 80rpx;
  font-weight: bold;
  font-size: 20px;
  text-align: center;
}
.search {
  width: 100%;
  height: 100rpx;
  flex-direction: row;
  padding: 0 16rpx;
  margin-top: 30rpx;
  margin-bottom: 15rpx;
  display: flex;
  justify-content: flex-start;
  background-color: white;
}
.search-icon {
  width: 10%;
  justify-content: center;
  margin-left: 16rpx;
}
.search-input {
  width: 85%;
  height: 80rpx;
  background-color: #f5f5f5;
  border-radius: 40rpx;
  margin-left: 16rpx;
  align-items: flex-start;
  justify-content: center;
}
.search-input text {
  color: gray;
  margin-left: 30rpx;
  justify-content: flex-start;
  align-items: center;
  display: flex;
  align-content: center;
  height: 100%;
}

view {
  overflow: hidden;
  box-sizing: border-box;
  flex-wrap: wrap;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
}
.list-nav{
  margin: 0rpx 25rpx;
  background: #ffffff;
  margin-bottom: 25rpx;
}
.nav-title{
  background: #ffffff;
  margin: 0rpx;
  height: 90rpx;
  font-size: 28rpx;
  padding: 0rpx 15rpx 0rpx 0rpx;
  align-content: center;
  align-items: center;
  border-bottom: 4rpx solid #5f0707;
  color: #666666;
  position: relative;
}
.nav-tab{
  margin-top: 25rpx;
  padding: 0rpx 20rpx;
  padding-bottom: 25rpx;
}
.img-list{
  width: 32.333%;
  height: 160rpx;
  position: relative;
}
.img-01{
  height:160rpx; 
  width:100%;
  border-radius: 8rpx;
}
.keshu{
  color: #ffffff;
  position: absolute;
  bottom: 0;
  width: 100%;
  display: flex;
  height: 100%;
  border-radius: 8rpx;
}
.d-row {
  display: flex;
  flex-direction: row;
}
.j-between {
  justify-content: space-between;
}

.more-nav-01{
  background: #ffffff;
  margin: 25rpx;
  border-radius: 8rpx;
}
.more-name{
  position: relative;
  height: 220rpx;
  width: 100%;
}
.more-img{
  width: 100%;
  height: 385rpx;
  display: flex;
}
.more-img-name{
  position: absolute;
  top: 0;
  padding-left: 35rpx;
  width: 100%;
  height: 100%;
  display: flex;
  font-size: 36rpx;
  font-weight: 600;
  background: rgba(145, 137, 137, 0.322);
  color: #ffffff;
  border-top-left-radius: 8rpx;
  align-items: flex-start;
  border-bottom-left-radius: 8rpx;
}
.h1-txt{
  font-size: 36rpx;
  font-weight: 600;
}
.h6-txt{
  font-size: 30rpx;
  font-weight: normal;
  margin-top: 10rpx;
}
